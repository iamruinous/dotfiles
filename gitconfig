# This is Git's per-user configuration file.
[core]
	user = Jade Meskill
	email = jade.meskill@gmail.com
	editor = /usr/local/bin/nvim
	excludesfile = /Users/jmeskill/.gitignore_global
[push]
	default = tracking
[user]
	name = Jade Meskill
	email = jade.meskill@gmail.com
	signkey = CA5500A8F3A920A0
[mergetool "Kaleidoscope"]
	cmd = ksdiff --merge --output \"$MERGED\" --base \"$BASE\" -- \"$LOCAL\" --snapshot \"$REMOTE\" --snapshot
	trustexitcode = true
[merge]
	tool = Kaleidoscope
[difftool "Kaleidoscope"]
	cmd = ksdiff --partial-changeset --relative-path \"$MERGED\" -- \"$LOCAL\" \"$REMOTE\"
[difftool]
	prompt = false
[mergetool]
	prompt = false
[diff]
	tool = Kaleidoscope
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[branch]
	autosetupmerge = true
[commit]
	gpgsign = true
[remote "origin"]
	prune = true
